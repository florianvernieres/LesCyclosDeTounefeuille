{% extends 'base.html.twig' %}

{% block title %}Hello MembreController!{% endblock %}

{% block body %}
    <div class="container">
        <h2>les membres du club</h2>
        <div class="row flex">
            {% for membre in membres %}
                <div class="col-3">
                    <div class="card">
                        <h5 class="card-title">
                        </h5>
                        <p class="card-text">
                            {{ membre.nom }}
                            {{ membre.prenom }}
                            {{ membre.age }} ans
                        </p>
                    </div>
                </div>
            {% endfor %}
        </div>
    </div>
{% endblock %}
